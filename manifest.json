{
  "description": "proxy hook",
  "version": "0.0.1",
  "name": "hooker",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
      "scripts": [
          "esprima.js",
          "escodegen.js",
          "estraverse.js",
          "background.js"
      ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' *://*/*; object-src 'self'"
}