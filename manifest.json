{
  "description": "proxy hook",
  "version": "0.0.1",
  "name": "hooker",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "dist/background.bundle.js"
    ]
  },
  "web_accessible_resources": [
    "hook.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' *://*/*; object-src 'self'"
}